{
  "name": "divinecraft",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --mode development --env development --config webpack.config.dev.cjs",
    "build": "npx webpack --config webpack.config.prod.cjs",
    "link-assets": "rm -r ./build/assets; cp -r ./static/assets ./build/assets",
    "init:amodx:threads": "node ./scripts/init-lib.js ./libs/amodx/threads \"@amodx\"",
    "init:amodx:core": "node ./scripts/init-lib.js ./libs/amodx/core \"@amodx\"",
    "init:amodx:audio": "node ./scripts/init-lib.js ./libs/amodx/audio \"@amodx\"",
    "init:amodx:rng": "node ./scripts/init-lib.js ./libs/amodx/rng \"@amodx\"",
    "init:amodx:schemas": "node ./scripts/init-lib.js ./libs/amodx/schemas \"@amodx\"",
    "init:amodx:ncs": "node ./scripts/init-lib.js ./libs/amodx/ncs \"@amodx\"",
    "init:amodx:math": "node ./scripts/init-lib.js ./libs/amodx/math \"@amodx\"",
    "init:amodx:controls": "node ./scripts/init-lib.js ./libs/amodx/controls \"@amodx\"",
    "init:amodx:binary": "node ./scripts/init-lib.js ./libs/amodx/binary \"@amodx\"",
    "init:amodx:elm": "node ./scripts/init-lib.js ./libs/amodx/elm \"@amodx\"",
    "init:amodx:indexdb": "node ./scripts/init-lib.js ./libs/amodx/indexdb \"@amodx\"",
    "init:amodx:all": "npm run init:amodx:threads ; npm run init:amodx:core  ; npm run init:amodx:audio ; npm run init:amodx:rng  ; npm run init:amodx:indexdb ; npm run init:amodx:schemas ; npm run init:amodx:ncs ; npm run init:amodx:math ; npm run init:amodx:controls ; npm run init:amodx:binary ; npm run init:amodx:elm",
    "init:divinevoxel:vlox": "node ./scripts/init-lib.js ./libs/divinevoxel/vlox \"@divinevoxel\"",
    "init:divinevoxel:vlox-babylon": "node ./scripts/init-lib.js ./libs/divinevoxel/vlox-babylon \"@divinevoxel\"",
    "init:divinevoxel:all": "npm run init:divinevoxel:vlox ; npm run init:divinevoxel:vlox-babylon",
    "init:dvegames:vlox": "node ./scripts/init-lib.js ./libs/dvegames/vlox \"@dvegames\"",
    "init:dvegames:vlox-tools": "node ./scripts/init-lib.js ./libs/dvegames/vlox-tools \"@dvegames\"",
    "init:dvegames:all": "npm run init:dvegames:vlox ; npm run init:dvegames:vlox-tools",
    "init:all": "npm run init:amodx:all ; npm run init:divinevoxel:all ; npm run init:dvegames:all "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "alea": "^1.0.1",
    "simplex-noise": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@babylonjs/core": "^7.34.2",
    "dat.gui": "^0.7.9"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@webgpu/types": "^0.1.51",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss-loader": "^8.1.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "workspaces": [
    "libs/amodx/threads/dist",
    "libs/amodx/core/dist",
    "libs/amodx/schemas/dist",
    "libs/amodx/ncs/dist",
    "libs/amodx/math/dist",
    "libs/amodx/controls/dist",
    "libs/amodx/binary/dist",
    "libs/amodx/elm/dist",
    "libs/divinevoxel/vlox/dist",
    "libs/divinevoxel/vlox-babylon/dist",
    "libs/dvegames/vlox/dist",
    "libs/dvegames/vlox-tools/dist",
    "libs/amodx/indexdb/dist",
    "libs/amodx/rng/dist"
  ]
}
